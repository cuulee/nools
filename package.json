  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:C2FO/nools.git"
  },
  "dependencies": {
    "declare.js": "~0.0.3",
    "extended": "~0.0.3",
    "is-extended": "~0.0.4",
    "string-extended": "~0.0.3",
    "array-extended": "~0.0.4",
    "date-extended": "~0.0.3",
    "function-extended": "~0.0.3",
    "object-extended": "~0.0.3",
    "promise-extended": "~0.0.3",
    "arguments-extended": "~0.0.3",
    "leafy": "~0.0.3",
    "ht": "~0.0.2",
    "commander": "~1.1.1",
    "uglify-js": "~2.4.24"
  },
  "devDependencies": {
    "coddoc": "0.0.3",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.10.0",
    "grunt": "~0.4.1",
    "grunt-browserify": "1.2.4",
    "grunt-contrib-jshint": "~0.5.3",
    "grunt-contrib-uglify": "~0.2.0",
    "grunt-eslint": "^19.0.0",
    "grunt-exec": "^0.4.5",
    "jison": "~0.4.17",
    "mocha": "^3.2.0"
  },
  "testling": {
    "files": "test/browserling.js",
    "browsers": [
      "ie/8..latest",
      "chrome/20..latest",
      "firefox/14..latest",
      "safari/latest",
      "iphone/6",
      "ipad/6"
    ]
  },
  "homepage": "http://c2fo.github.com/nools",
  "author": "Doug Martin (http://c2fo.com)",
  "main": "index.js",
  "scripts": {
    "create-doc": "rm -rf docs/* && ./node_modules/coddoc/bin/coddoc -d ./lib -f multi-html",
    "test": "npm run eslint && npm run mocha",
    "eslint": "./node_modules/eslint/bin/eslint.js lib/. test/.",
    "eslint-fix": "./node_modules/eslint/bin/eslint.js lib/. test/. --fix",
    "mocha": "mocha test/ --recursive"
  },
  "directories": {
    "lib": "lib"
  },
  "engines": {
    "node": ">= 0.6.1"
  }
